        <dc:title>古文观止</dc:title>
        <dc:creator opf:file-as="吴楚材 &amp; 吴调侯" opf:role="aut">吴楚材</dc:creator>
        <dc:creator opf:file-as="吴楚材 &amp; 吴调侯" opf:role="aut">吴调侯</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.9.9) [http://calibre-ebook.com]</dc:contributor>
